                            <rdfs:label>meta/void</rdfs:label>
                        </dct:IMT>
                    </dct:format>
               <dct:title>voiD description</dct:title>
            </dcat:Distribution>
        </dcat:distribution>
        <dcat:distribution>
            <dcat:Distribution>
                <dcat:accessURL rdf:resource="http://lobid.org/download/dumps/DE-605/hbzlod.nt.tar.bz2"></dcat:accessURL>
                    <dct:format>
                        <dct:IMT>
