               <dct:title>Link to an example data item</dct:title>
            </dcat:Distribution>
        </dcat:distribution>
        <dcat:distribution>
            <dcat:Distribution>
                <dcat:accessURL rdf:resource="http://lobid.org/dataset/resource/about.ttl"></dcat:accessURL>
